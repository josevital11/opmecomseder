	<queryString>
		<![CDATA[SELECT
 a.id_aplic,
 c.id_cir,
 c.data_cir,
 a.cod_tiss,
 a.nome_comerc,
 a.desc_forn,
 a.preco_unit,

 f.nome_forn
FROM cirurgia c
INNER JOIN aplicado a ON a.id_cir = c.id_cir
INNER JOIN fornecedor f ON f.id_forn = a.id_forn

ORDER BY c.data_cir]]>
