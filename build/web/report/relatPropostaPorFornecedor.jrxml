	<queryString>
		<![CDATA[SELECT
     b.`mat_benef`,
     b.`nome_benef`,
     o.`id_prop`,
     p.`cod_proc`,
     p.`nome_proc`,
     c.`data_cir`,
     o.`id_prop`,

     o.`id_opme`,
     o.`id_forn`,
     o.`cod_tiss`,
     o.`nome_comerc`,
     o.`unidade`,
     o.`qtd`,
     o.`preco_unit`,
     o.`preco_total`,
     o.`desc_forn`,
     f.`nome_forn`

FROM `cirurgia` c
INNER JOIN `beneficiario` b ON c.`id_benef` = b.`id_benef`
     INNER JOIN `procedimento` p ON c.`id_proc` = p.`id_proc`
     INNER JOIN `envia_proposta` o ON c.`id_cir` = o.`id_cir`
     INNER JOIN `fornecedor` f ON o.`id_forn` = f.`id_forn`]]>
	</queryString>
