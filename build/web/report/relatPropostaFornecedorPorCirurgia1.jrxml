	<queryString>
		<![CDATA[SELECT
     b.mat_benef,
     b.nome_benef,
     c.data_cir,
     f.nome_forn,
     p.cod_proc,
     p.nome_proc,
     o.cod_tiss,
     o.nome_comerc,
     o.unidade,
     o.qtd,
     o.preco_unit,
     o.preco_total,
     o.desc_forn,
     o.id_prop,
     o.nomeUserLogado
FROM cirurgia c
     INNER JOIN proposta o ON c.id_cir = o.id_cir
     INNER JOIN beneficiario b ON c.id_benef = b.id_benef
     INNER JOIN procedimento p ON c.id_proc = p.id_proc
     INNER JOIN fornecedor f ON o.id_forn = f.id_forn]]>
	</queryString>
