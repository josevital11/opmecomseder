	<queryString language="SQL">
		<![CDATA[SELECT
     c.id_cir,
     c.data_cir,
     c.id_benef,
     b.mat_benef,
     b.nome_benef,
     c.id_proc,
     p.id_proc,
     p.nome_proc,
     c.id_prest,
     r.id_prest,
     r.nome_prest
     FROM cirurgia c
     INNER JOIN beneficiario b ON b.id_benef = c.id_benef
     INNER JOIN procedimento  p ON c.id_proc = p.id_proc
     INNER JOIN prestador  r ON c.id_prest = r.id_prest
 ORDER BY
     b.nome_benef ASC]]>
	</queryString>
