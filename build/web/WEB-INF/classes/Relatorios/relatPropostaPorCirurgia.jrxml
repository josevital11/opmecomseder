	<queryString>
		<![CDATA[SELECT
     o.`id_orc`,
     c.`data_cir`,
     o.`cod_tiss`,
     p.`cod_proc`,
     p.`nome_proc`,
     b.`mat_benef`,
     b.`nome_benef`,
     o.`cod_tiss`,
     o.`nome_comerc`,
     o.`unidade`,
     o.`qtd`,
     o.`preco_unit`,
     o.`preco_total`,
     pr.`id_prop`,
     pr.`cod_tiss`,
     pr.`nome_comerc`,
     pr.`unidade`,
     pr.`qtd`,
     pr.`preco_unit`,
     pr.`preco_total`
