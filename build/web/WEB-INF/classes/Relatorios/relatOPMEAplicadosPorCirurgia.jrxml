	<queryString>
		<![CDATA[SELECT
 a.id_aplic,
 c.data_cir,
 a.cod_tiss,
 p.cod_proc,
 p.nome_proc,
 b.mat_benef,
 b.nome_benef,
 a.cod_tiss,
 a.nome_comerc,
 a.unidade,
 a.qtd,
 a.preco_unit,
 a.preco_total,
 a.desc_forn,
 f.nome_forn
FROM cirurgia c
INNER JOIN beneficiario b ON b.id_benef = c.id_benef
INNER JOIN procedimento  p ON c.id_proc = p.id_proc
INNER JOIN aplicado a ON c.id_cir = a.id_cir
INNER JOIN fornecedor f ON f.id_forn = a.id_forn]]>
	</queryString>
